name: ewars_plus

entry_points:
  train:
    parameters:
      train_data: str
      model: str
    command: "python main.py train {train_data} {geojson}"
  predict:
    parameters:
      historic_data: str
      future_data: str
      model: str
      out_file: str
    command: "python main.py predict  {model} {historic_data} {future_data} {out_file}"